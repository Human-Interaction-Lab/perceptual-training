{
  "name": "perceptual-training-app",
  "version": "1.0.0",
  "description": "Audio Perception Training Application",
  "scripts": {
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm start",
    "client": "cd frontend && npm start",
    "start": "npm run dev",
    "test:e2e": "playwright test final-comprehensive.spec.js --config=playwright.config.manual.js",
    "test:e2e:all-phases": "playwright test working-phases.spec.js --config=playwright.config.manual.js",
    "test:e2e:ui": "playwright test final-comprehensive.spec.js --config=playwright.config.manual.js --ui",
    "test:e2e:headed": "playwright test final-comprehensive.spec.js --config=playwright.config.manual.js --headed",
    "test:e2e:debug": "playwright test final-comprehensive.spec.js --config=playwright.config.manual.js --debug",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:setup": "node e2e-tests/create-phase-users.js create",
    "test:e2e:cleanup": "node e2e-tests/create-phase-users.js cleanup",
    "test:e2e:comprehensive": "playwright test comprehensive-full-stack.spec.js --config=playwright.config.manual.js",
    "test:e2e:simple": "playwright test simple-activity-capture.spec.js --config=playwright.config.manual.js",
    "test:e2e:fresh": "playwright test fresh-user-capture.spec.js --config=playwright.config.manual.js",
    "test:e2e:manual": "playwright test manual-activity-capture.spec.js --config=playwright.config.manual.js",
    "test:e2e:journey": "playwright test comprehensive-user-journey.spec.js --config=playwright.config.manual.js",
    "test:e2e:journey:headed": "playwright test comprehensive-user-journey.spec.js --config=playwright.config.manual.js --headed",
    "test:e2e:journey:debug": "playwright test comprehensive-user-journey.spec.js --config=playwright.config.manual.js --debug",
    "test:e2e:screenshots": "playwright test complete-screenshot-journey.spec.js --config=playwright.config.manual.js",
    "test:e2e:screenshots:headed": "playwright test complete-screenshot-journey.spec.js --config=playwright.config.manual.js --headed"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@playwright/test": "^1.53.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "express": "^4.21.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "lucide": "^0.460.0",
    "mongoose": "^8.8.1",
    "tailwind-merge": "^2.5.4",
    "y18n": "^5.0.8"
  }
}
